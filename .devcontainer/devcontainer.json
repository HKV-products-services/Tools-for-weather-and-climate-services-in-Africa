{
  "name": "Python 3",
  "image": "mcr.microsoft.com/devcontainers/python:1-3.10-bookworm",
  "postCreateCommand": "python3 -m pip install -r requirements.txt",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md"
      ]
    },
    "vscode": {
      "jupyter.widgetScriptSources": [
        "unpkg.com",
        "jsdelivr.com"
      ]
    },
    "extensions": [
      "ms-toolsai.jupyter",
      "ms-python.python",
      "ms-toolsai.jupyter-keymap",
      "ms-toolsai.jupyter-renderers"
    ]
  }
}
